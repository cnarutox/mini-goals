/* pages/mine/mine.wxss */
.myNavigator{
  display: flex;
  flex-direction: column;
  height: 50rpx;
}

.navigatorPkg{
  display: flex;
  flex-direction: row;
  width: 100%;
  border: 5rpx solid red;
  align-items: flex-start;
  justify-content: flex-start;
}

.navigator{
  border: 5rpx solid black;
  width: 100%;
}

.iconMine{
  justify-content: flex-start;
}


.container{
  display: flex;
  flex-direction: column;
}

.userinfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  background: yellowgreen;
  width: 100%;
}

.userinfoview{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
  width: 100%;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname{
  color: #fff;
}

.separate {
  width: 100%;
  height: 20rpx;
  background-color: lightgray;
}

.mainpage{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  background: yellow;
  width: 100%;
}

.icontext{
  display: flex;
  flex-direction: row;
  margin: 20rpx;
  padding: 20rpx;
}

.iconmine{
  margin-right: 10rpx;
}
