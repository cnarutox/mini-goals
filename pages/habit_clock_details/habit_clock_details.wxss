button::after{ border: none; }
.btn-qiandao{width:200rpx;line-height:80rpx;color:#fff;font-size:30rpx;text-align:center;border: none;outline: none;background:#02d7d7;margin-left:275rpx;position: relative;top:0rpx;}
.btn-qiandao1{
    border: none;width:200rpx;line-height:80rpx;color:#fff;font-size:30rpx;text-align:center;border: none;outline: none;background:#e1e1e1;margin-left:275rpx;position: relative;top:0rpx;}
.headers_wrapper{width:650rpx;height:200rpx;background: linear-gradient(to right, #48fefe , #09dbdb);overflow: hidden;zoom: 1;padding:25rpx 50rpx;margin-top: 0}
.for_headerleft{float: left;width:210rpx;
    height: 150rpx;margin-top: 20rpx;}
.for_headerleft image{width: 100%;height: 100%;}
.for_headerright{
    width: 430rpx;
    float: right;font-size:30rpx;
    margin-top:22rpx;
    color: #fff;
}
.for_headerright_list{
    font-size:36rpx;
    font-weight: bold;
    line-height: 50rpx;
}
.for_headerright_list1 span{
    font-weight: bold;font-size:30rpx;padding:0 5rpx;
}
.for_headerright_list2{
    line-height:50rpx;
    font-size:24rpx;
}
.for_headerright_list2 span{
    font-weight: bold;padding:5rpx;
    font-size:30rpx;
}
.for_headerright_list1{
    line-height: 50rpx;
}
.calen_header{
    width:650rpx;
    margin: 0 auto;
    margin-top: 30rpx;
}
.calenbox{
    color: #00DFE1;width: 100%;text-align: center;line-height: 50rpx;font-size:30rpx;height: 50rpx;margin-top: 10rpx;
}
.calendar_month_prev{
    width: 80rpx;height: 80rpx;
    float: left;
    background: none;
    border: none;
}
.calendar_month_prev image{
    width: 25rpx;height: 50rpx;
}
.calendar_month_next{
    width: 80rpx;height: 80rpx;
    float: right;
    background: none;
    border: none;
}
.calendar_month_next image{
    width: 25rpx;height: 50rpx;
    transform: rotate(180deg);
}
/*微信小程序签到wxss--2018-5-25 杨京葛*/
.calendar_wrapper{
    width:650rpx;
    height: 560rpx;
    margin: 20rpx 50rpx;
    font-size:24rpx;
    color: #2d2d2d;
    overflow: hidden;
    zoom: 1;
}
/*日历大小自定义*/
.week_day{
    width: 630rpx;
    overflow: hidden;
    margin:0 10rpx;
    zoom: 1;
    height:90rpx;
}
.weeklist{
    width:90rpx;
    height:60rpx;
    line-height:90rpx;
    text-align: center;
    float: left;
}
/*星期定义*/
.month_day{
    width: 630rpx;
    overflow: hidden;
    margin:0 10rpx;
    zoom: 1;
    height:90rpx;
}
.day_list{
    width:90rpx;
    height:90rpx;
    line-height:90rpx;
    text-align: center;
    float: left;
    position: relative;
}
/*日历输出定义*/
.day_hover{background:url(data:image/png;base64,) no-repeat right bottom;
    background-size:25rpx 25rpx;
    position: absolute;
    top: 0;
    left: 0;
    width:90rpx;
    height:90rpx;
}
/*签到样式*/
.maskbox{
    background: #000;
    width: 100%;
    height: 100%;
    opacity: 0.3;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5;
    display: none;
}
.qdbox{
    display:none;
    padding:100rpx 25rpx;
    width:500rpx;
    background:#fff;
    position:fixed;
    z-index:10;
    top:150rpx;
    left:100rpx;
    height:420rpx;
    overflow: hidden;
    zoom: 1;
    display: none;
    }
.qdbox .text-green{color:#02d7d7;}
.hover_powerimg image{
    width: 100rpx;
    height: 100rpx;
    margin:30rpx 200rpx;
    margin-bottom: 50rpx;
}
.hover_title_s{
    width:100%;
    text-align: center;
    line-height: 50rpx;
    font-size:36rpx;

}
.hover_title{
    text-align: center;
    font-size:24rpx;
    line-height:100rpx;
}
.hover_powers{
    width: 100%;
    text-align: center;
}